<mvc:View
    height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:ff="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt"
    xmlns:t="sap.ui.table"
    xmlns:trm="sap.ui.table.rowmodes"
    controllerName="parceltracking.controller.Home"
>
    <tnt:ToolPage id="toolPage">
        <tnt:header>
            <f:ShellBar
                id="_IDGenShellBar1"
                title="Package Tracking Service"
                showMenuButton="true"
                homeIcon="img/logo.png"
                showCopilot="true"
                showNotifications="true"
                menuButtonPressed="onMenuButtonPress"
                notificationsNumber="2"
            >
                <f:menu>
                    <Menu id="_IDGenMenu1">
                        <MenuItem
                            id="_IDGenMenuItem1"
                            text="Flight booking"
                            icon="sap-icon://flight"
                        />
                        <MenuItem
                            id="_IDGenMenuItem2"
                            text="Car rental"
                            icon="sap-icon://car-rental"
                        />
                    </Menu>
                </f:menu>
                <f:profile>
                    <Avatar
                        id="_IDGenAvatar1"
                        initials="UI"
                    />
                </f:profile>
            </f:ShellBar>
        </tnt:header>
        <tnt:sideContent>
            <core:Fragment
                fragmentName="parceltracking.view.SideNavigation"
                id="fragment"
                type="XML"
            />
        </tnt:sideContent>
        <tnt:mainContents>
            <NavContainer
                id="pageContainer"
                initialPage="Sender_Overview"
            >
                <pages>
                    <ScrollContainer
                        id="Sender_Overview"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                    >
                        <core:Fragment
                            fragmentName="parceltracking.view.SenderOverview"
                            type="XML"
                        />
                    </ScrollContainer>
                    <ScrollContainer
                        id="Sender_Create"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                    >
                        <core:Fragment
                            fragmentName="parceltracking.view.SenderCreate"
                            type="XML"
                        />
                    </ScrollContainer>
                    <ScrollContainer
                        id="Receiver_Overview"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                    >
                        <content>
                            <VBox
                                id="_IDGenVBox1"
                                fitContainer="true"
                            >
                                <ff:SimpleForm
                                    id="SimpleFormDisplay480"
                                    editable="false"
                                    layout="ResponsiveGridLayout"
                                    title="Address"
                                    labelSpanL="4"
                                    labelSpanM="4"
                                    emptySpanL="0"
                                    emptySpanM="0"
                                    columnsL="2"
                                    columnsM="2"
                                >
                                    <ff:content>
                                        <core:Title
                                            id="_IDGenTitle1"
                                            text="Office"
                                        />
                                        <Label
                                            id="_IDGenLabel1"
                                            text="Name"
                                        />
                                        <Text
                                            id="_IDGenText2"
                                            text="Red Point Stores"
                                        />
                                        <Label
                                            id="_IDGenLabel2"
                                            text="Street/No."
                                        />
                                        <Text
                                            id="_IDGenText3"
                                            text="Main St 1618"
                                        />
                                        <Label
                                            id="_IDGenLabel3"
                                            text="ZIP Code/City"
                                        />
                                        <Text
                                            id="_IDGenText4"
                                            text="31415 Maintown"
                                        />
                                        <Label
                                            id="_IDGenLabel4"
                                            text="Country"
                                        />
                                        <Text
                                            id="_IDGenText5"
                                            text="Germany"
                                        />
                                        <core:Title
                                            id="_IDGenTitle2"
                                            text="Online"
                                        />
                                        <Label
                                            id="_IDGenLabel5"
                                            text="Web"
                                        />
                                        <Text
                                            id="_IDGenText6"
                                            text="http://www.sap.com"
                                        />
                                        <Label
                                            id="_IDGenLabel6"
                                            text="Twitter"
                                        />
                                        <Text
                                            id="_IDGenText7"
                                            text="@sap"
                                        />
                                    </ff:content>
                                    <ff:layoutData>
                                        <FlexItemData
                                            id="_IDGenFlexItemData1"
                                            shrinkFactor="0"
                                            backgroundDesign="Solid"
                                            styleClass="sapContrastPlus"
                                        />
                                    </ff:layoutData>
                                </ff:SimpleForm>
                                <IconTabBar
                                    id="_IDGenIconTabBar1"
                                    upperCase="true"
                                    expandable="false"
                                    applyContentPadding="true"
                                    stretchContentHeight="true"
                                    class="sapUiResponsiveContentPadding"
                                >
                                    <items>
                                        <IconTabFilter
                                            id="_IDGenIconTabFilter1"
                                            key="balances"
                                            text="Balances"
                                        />
                                        <IconTabFilter
                                            id="_IDGenIconTabFilter2"
                                            key="compare"
                                            text="Compare"
                                        />
                                    </items>
                                    <content>
                                        <t:AnalyticalTable
                                            id="_IDGenAnalyticalTable1"
                                            selectionMode="MultiToggle"
                                        >
                                            <t:rowMode>
                                                <trm:Auto
                                                    id="_IDGenAuto1"
                                                    rowContentHeight="32"
                                                />
                                            </t:rowMode>
                                            <t:toolbar>
                                                <OverflowToolbar
                                                    id="_IDGenOverflowToolbar1"
                                                >
                                                    <ToolbarSpacer
                                                        id="_IDGenToolbarSpacer1"
                                                    />
                                                    <SearchField
                                                        id="_IDGenSearchField1"
                                                        width="12rem"
                                                    />
                                                    <ToolbarSpacer
                                                        id="_IDGenToolbarSpacer2"
                                                        width="1rem"
                                                    />
                                                    <SegmentedButton
                                                        id="_IDGenSegmentedButton1"
                                                    >
                                                        <items>
                                                            <SegmentedButtonItem
                                                                id="_IDGenSegmentedButtonItem1"
                                                                icon="sap-icon://table-view"
                                                            />
                                                            <SegmentedButtonItem
                                                                id="_IDGenSegmentedButtonItem2"
                                                                icon="sap-icon://bar-chart"
                                                            />
                                                        </items>
                                                    </SegmentedButton>
                                                    <ToolbarSpacer
                                                        id="_IDGenToolbarSpacer3"
                                                        width="1rem"
                                                    />
                                                    <Button
                                                        id="_IDGenButton1"
                                                        icon="sap-icon://group-2"
                                                        type="Transparent"
                                                    />
                                                    <Button
                                                        id="_IDGenButton2"
                                                        icon="sap-icon://action-settings"
                                                        type="Transparent"
                                                    />
                                                </OverflowToolbar>
                                            </t:toolbar>
                                            <t:columns>
                                                <t:AnalyticalColumn
                                                    id="_IDGenAnalyticalColumn1"
                                                />
                                                <t:AnalyticalColumn
                                                    id="_IDGenAnalyticalColumn2"
                                                />
                                                <t:AnalyticalColumn
                                                    id="_IDGenAnalyticalColumn3"
                                                />
                                                <t:AnalyticalColumn
                                                    id="_IDGenAnalyticalColumn4"
                                                />
                                            </t:columns>
                                        </t:AnalyticalTable>
                                    </content>
                                    <layoutData>
                                        <FlexItemData
                                            id="_IDGenFlexItemData2"
                                            growFactor="1"
                                            baseSize="0%"
                                        />
                                    </layoutData>
                                </IconTabBar>
                            </VBox>
                        </content>
                    </ScrollContainer>
                </pages>
            </NavContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
